: ?test_string().

?test_string() {
    string s = "abc def";
    s = "abc fed" -> fail();
    s = "abc def";
    string e = "abc def";
    s = e;
    string f = "abc fed";
    s = f -> fail();
}

// 'т'
// 'δ'
: ?test_string_unicode(char).
?test_string_unicode(ch) {
    string s = "Жертвоприношение";
    c ~ char;
    s by_idx 3 = c;
    ch = c;
    length of s = 16;
}
: ?test_string_unicode(ch) {
    ch = 'δ';
}